{
  "name": "boilerplate-lp-2024",
  "version": "0.1.0",
  "dependencies": {
    "next": "14.2.4",
    "react": "^18",
    "react-dom": "^18",
    "serve": "^14.2.3"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-tailwindcss": "^3.17.3",
    "fixpack": "^4.0.0",
    "npm-run-all2": "^6.2.0",
    "postcss": "^8",
    "prettier": "^3.3.2",
    "prettier-plugin-organize-imports": "^3.2.4",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.5"
  },
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "fix": "run-p --continue-on-error fix:*",
    "fix:css": "run-s fix:css:prettier",
    "fix:css:prettier": "prettier './**/*.css' --write",
    "fix:js": "run-s fix:js:eslint fix:js:prettier",
    "fix:js:eslint": "next lint --fix",
    "fix:js:prettier": "prettier './**/*.{js,jsx,mjs,ts,tsx}' --write",
    "fix:json": "prettier './**/*.json' --write",
    "fix:md": "prettier './**/*.md' --write",
    "fix:pack": "fixpack",
    "lint": "run-p --continue-on-error lint:*",
    "lint:css": "run-p --continue-on-error lint:css:*",
    "lint:css:prettier": "prettier './**/*.css' --check",
    "lint:js": "run-p --continue-on-error lint:js:*",
    "lint:js:eslint": "next lint",
    "lint:js:prettier": "prettier './**/*.{js,jsx,mjs,ts,tsx}' --check",
    "lint:js:tsc": "tsc --noEmit",
    "lint:json": "prettier './**/*.json' --check",
    "lint:md": "prettier './**/*.md' --check",
    "lint:pack": "fixpack --dryRun",
    "start": "serve ./out"
  }
}
